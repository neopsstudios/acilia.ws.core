{
    "name": "aciliainternet/wscore",
    "description": "Widestand Core bundle",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2.3",
        "ext-dom": "*",
        "ext-gmp": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-zlib": "*",
        "symfony/asset": "*",
        "symfony/console": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^1.1",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/lock": "*",
        "symfony/monolog-bundle": "*",
        "symfony/orm-pack": "*",
        "symfony/process": "*",
        "symfony/security-bundle": "*",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/yaml": "*",
        "symfony/maker-bundle": "*",
        "sensio/framework-extra-bundle": "^5.1",
        "twig/extensions": "^1.5",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "piwik/device-detector": "^3.12",
        "intervention/image": "^2.4",
        "erusev/parsedown": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "WS\\Core\\": "src/Core"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "require": "4.4.*",
            "allow-contrib": false
        }
    }
}
